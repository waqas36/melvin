<odoo>
    <data>
        <record id="account_analytic_line_form_ext" model="ir.ui.view">
            <field name="name">account.analytic.line.form.ext</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_form"/>
            <field name="arch" type="xml">
                <field name="date" position="before">
                    <field name="employee_id"/>
                    <field name="supervisor"/>
                    <field name="company"/>
                </field>
                <field name="amount" position="attributes">
                    <attribute name="invisible">True</attribute>
                </field>
                <field name="unit_amount" position="attributes">
                    <attribute name="invisible">True</attribute>
                </field>
                <!--<field name="project_id" position="attributes">-->
                    <!--<attribute name="required">False</attribute>-->
                <!--</field>-->
                <!--<field name="account_id" position="attributes">-->
                    <!--<attribute name="required">False</attribute>-->
                <!--</field>-->

                <xpath expr="//form/sheet/group/group[2]" position="inside">
                    <field name="time_start" widget="timepicker"/>
                    <field name="time_end" widget="timepicker"/>
                    <field name="hour_worked" readonly="True"   />
                    <field name="rate"/>
                    <field name="charges"/>
                    <field name="remarks"/>
                </xpath>
            </field>
        </record>
        <record id="account_analytic_line_tree_ext" model="ir.ui.view">
            <field name="name">account.analytic.line.tree.ext</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="hr_timesheet.timesheet_view_tree_user"/>
            <field name="arch" type="xml">
                <field name="unit_amount" position="after">
                    <field name="rate"/>
                    <field name="supervisor"/>
                    <field name="company"/>
                    <field name="remarks"/>
                    <field name="employee_id"/>
                    <field name="name"/>
                    <field name="date"/>
                    <field name="time_start"/>
                    <field name="time_end"/>
                </field>
            </field>
        </record>

    </data>
</odoo>